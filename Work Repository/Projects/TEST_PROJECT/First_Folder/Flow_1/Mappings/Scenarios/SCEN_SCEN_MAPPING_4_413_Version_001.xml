  DIST_1_1.SUBS_STRT_DATE_ID ,
  DIST_1_1.SUBS_OFR_PROD_ID ,
  DIST_1_1.SUBS_NAME ,
  DIST_1_1.SUBS_SRVC_NO ,
  DIST_1_1.INV_ID ,
  DIST_1_1.SUBS_SALE_DATE_ID ,
  DIST_1_1.SUBS_SALE_WRK_ORG_ID ,
