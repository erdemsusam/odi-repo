  ) 
SELECT 
  DIST_1_1.HESAP_NO ,
  DIST_1_1.FATURA_NO ,
  DIST_1_1.ODEME_DONEMI ,
  DIST_1_1.FATURA_DURUM ,
  DIST_1_1.ORJINAL_TUTAR ,
  DIST_1_1.FATURA_TIPI ,
  DIST_1_1.SON_ODEME_TARIHI ,
  DIST_1_1.ODEME_TARIHI ,
  DIST_1_1.ODEMESURESI ,
  DIST_1_1.SUBS_ID ,
