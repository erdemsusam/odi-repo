  SUBS_SALE_1_3_1_1.SUBS_SALE_WRK_ORG_ID AS SUBS_SALE_WRK_ORG_ID ,
  SUBS_SALE_1_3_1_1.SUBS_SALE_OFR_ID AS SUBS_SALE_OFR_ID ,
  WRK_ORG_DIM_1_7_1.WRK_ORG_NAME AS WRK_ORG_NAME ,
  WRK_ORG_DIM_1_7_1.WRK_ORG_CITY AS WRK_ORG_CITY ,
  WRK_ORG_DIM_1_7_1.WRK_ORG_TP_NAME AS WRK_ORG_TP_NAME ,
  WRK_ORG_DIM_1_7_1.WRK_ORG_ST AS WRK_ORG_ST ,
