  (CASE WHEN DIST_1_1.ODEMESURESI <= 0 THEN 1 ELSE 0 END) ,
  (CASE WHEN DIST_1_1.ODEMESURESI BETWEEN 1 AND 30 THEN 1 ELSE 0 END) ,
  (CASE WHEN DIST_1_1.ODEMESURESI BETWEEN 31 AND 60 THEN 1 ELSE 0 END) ,
  (CASE WHEN DIST_1_1.ODEMESURESI BETWEEN 61 AND 90 THEN 1 ELSE 0 END) ,
  (CASE WHEN DIST_1_1.ODEMESURESI BETWEEN 91 AND 120 THEN 1 ELSE 0 END) ,
  (CASE WHEN DIST_1_1.ODEMESURESI > 120 THEN 1 ELSE 0 END) ,
  BILL_STMT_CHRG_2_1.CHRG_CLS_CATG_CD ,
  BILL_STMT_CHRG_2_1.STMT_CHRG_GROSS_AMT ,
  BILL_STMT_CHRG_2_1.STMT_CHRG_YEAR_MONTH ,
  BILL_STMT_CHRG_2_1.STMT_CHRG_DESCR  
FROM 
  (
SELECT 
  DISTINCT 
  FATURA_1_5.HESAP_NO AS HESAP_NO ,
