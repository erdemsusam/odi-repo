  OFC_DIM_1_9_1.OFC_ID AS OFC_ID ,
  OFC_DIM_1_9_1.OFC_NAME AS OFC_NAME ,
  OFC_DIM_1_9_1.OFC_CITY AS OFC_CITY ,
  CASE
                     WHEN (    SUBS_DIM_1_1_1.SUBS_NAME LIKE '%12%'
                           AND SUBS_DIM_1_1_1.SUBS_NAME NOT LIKE '%128%')
                     THEN
                        12
